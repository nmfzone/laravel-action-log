{
    "name": "nmfzone/laravel-action-log",
    "description": "Give your application ability to log all action that have been made.",
    "homepage": "https://github.com/nmfzone/laravel-action-log",
    "keywords":
    [
        "action",
        "logger",
        "action-log",
        "laravel"
    ],
    "authors": [
        {
            "name": "Nabil Muhammad Firdaus",
            "email": "123.nabil.dev@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.1",
        "illuminate/config": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "NMFCODES\\ActionLog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NMFCODES\\ActionLog\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NMFCODES\\ActionLog\\ActionLogServiceProvider"
            ],
            "aliases": {
                "ActionLog": "NMFCODES\\ActionLog\\ActionLogFacade"
            }
        }
    }
}
